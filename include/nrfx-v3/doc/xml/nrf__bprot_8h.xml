        <definition>NRF_STATIC_INLINE void nrf_bprot_nvm_blocks_protection_enable</definition>
        <argsstring>(NRF_BPROT_Type *p_reg, uint8_t group_idx, uint32_t block_mask)</argsstring>
        <name>nrf_bprot_nvm_blocks_protection_enable</name>
        <param>
          <type>NRF_BPROT_Type *</type>
          <declname>p_reg</declname>
        </param>
        <param>
          <type>uint8_t</type>
